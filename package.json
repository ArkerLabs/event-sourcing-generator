{
  "name": "event-sourcing-generator",
  "version": "1.0.1",
  "description": "Utils for generating CQRS + Event Sourcing classes for NestJS",
  "main": "./dist/event-sourcing-generator.js",
  "scripts": {
    "build": "rm -rf dist && tsc && cpx 'lib/templates/**/files/*.ts' dist/templates",
    "clean": "gulp clean:bundle",
    "start": "node dist/event-sourcing-generator.js",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "sudo npm i -g && es-generator -h"
  },
  "bin": {
    "es-generator": "./dist/event-sourcing-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkerlabs/eventsourcing-generator.git"
  },
  "keywords": [
    "es",
    "evnet",
    "sourcing",
    "cqrs",
    "nestjs"
  ],
  "author": "ArkerLabs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arkerlabs/eventsourcing-generator/issues"
  },
  "homepage": "https://github.com/arkerlabs/eventsourcing-generator#readme",
  "dependencies": {
    "@types/node": "^13.5.3",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "cpx": "^1.5.0"
  }
}
